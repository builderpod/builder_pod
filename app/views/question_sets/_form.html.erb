<%= form_with(model: question_set, local: true) do |form| %>
  <% if question_set.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(question_set.errors.count, "error") %> prohibited this question_set from being saved:</h2>

      <ul>
      <% question_set.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= form.label :oid %>
    <%= form.number_field :oid, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :task_id %>
    <%= form.number_field :task_id, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :task_oid %>
    <%= form.number_field :task_oid, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :original_task_oid %>
    <%= form.number_field :original_task_oid, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :set_id %>
    <%= form.number_field :set_id, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :interview_type %>
    <%= form.text_field :interview_type, class: 'form-control' %>
  </div>

  <div class="form-group">
    <% if question_set.persisted? %>
      <div class="float-right">
        <%= link_to 'Destroy', question_set, method: :delete, class: "text-danger", data: { confirm: 'Are you sure?' } %>
      </div>
    <% end %>

    <%= form.submit class: 'btn btn-primary' %>

    <% if question_set.persisted? %>
      <%= link_to "Cancel", question_set, class: "btn btn-link" %>
    <% else %>
      <%= link_to "Cancel", question_sets_path, class: "btn btn-link" %>
    <% end %>
  </div>
<% end %>
