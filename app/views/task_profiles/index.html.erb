


<div class="row">
  <div class="col-sm-6">
    <h1>Task_profiles</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_task_profile_path, class: 'btn btn-primary' do %>
    Add New Task profile
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Name</th>
    

            <th>Oid</th>
            <th>Description</th>
            <th>Xm descriptor</th>
            <th>Matchable</th>
            <th>Profileable</th>
            <th>Es score</th>
            <th>Default cost guide</th>
            <th>Keywords</th>
            <th>Text messaging name</th>
            <th>Rollover long desc</th>
            <th>Rollover short desc</th>
            <th>Sector</th>
            <th>Meta description</th>
            <th>Html title</th>
            <th>Content description</th>
            <th>Pwc</th>
            <th>Accept goal</th>
            <th>Multi zip</th>
            <th>Pre match type</th>
            <th>Bridge oid</th>
            <th>Bridge text</th>
            <th>Bridge link text</th>
            <th>Bridge link</th>
            <th>Bridge active</th>
            <th>Xm name</th>
            <th>Redirect to oid</th>
            <th>Sp display name</th>
            <th>Xm header descriptor</th>
            <th>Url name</th>
            <th>Consideration</th>
            <th>Search keywords</th>
            <th>Emc category oid</th>
            <th>Primary question set</th>
            <th>Location specific</th>
            <th>Commercial</th>
            <th>View first parent task</th>
            
      </tr>
    </thead>

    <tbody>
      <% @task_profiles.each do |task_profile| %>
        <%= content_tag :tr, id: dom_id(task_profile), class: dom_class(task_profile) do %>
          
            <td><%= link_to task_profile.name, task_profile %></td>
          

                      <td><%= task_profile.oid %></td>
                      <td><%= task_profile.description %></td>
                      <td><%= task_profile.xm_descriptor %></td>
                      <td><%= task_profile.matchable %></td>
                      <td><%= task_profile.profileable %></td>
                      <td><%= task_profile.es_score %></td>
                      <td><%= task_profile.default_cost_guide_id %></td>
                      <td><%= task_profile.keywords %></td>
                      <td><%= task_profile.text_messaging_name %></td>
                      <td><%= task_profile.rollover_long_desc %></td>
                      <td><%= task_profile.rollover_short_desc %></td>
                      <td><%= task_profile.sector_id %></td>
                      <td><%= task_profile.meta_description %></td>
                      <td><%= task_profile.html_title %></td>
                      <td><%= task_profile.content_description %></td>
                      <td><%= task_profile.pwc_id %></td>
                      <td><%= task_profile.accept_goal %></td>
                      <td><%= task_profile.multi_zip %></td>
                      <td><%= task_profile.pre_match_type_id %></td>
                      <td><%= task_profile.bridge_oid %></td>
                      <td><%= task_profile.bridge_text %></td>
                      <td><%= task_profile.bridge_link_text %></td>
                      <td><%= task_profile.bridge_link_id %></td>
                      <td><%= task_profile.bridge_active %></td>
                      <td><%= task_profile.xm_name %></td>
                      <td><%= task_profile.redirect_to_oid %></td>
                      <td><%= task_profile.sp_display_name %></td>
                      <td><%= task_profile.xm_header_descriptor %></td>
                      <td><%= task_profile.url_name %></td>
                      <td><%= task_profile.consideration_id %></td>
                      <td><%= task_profile.search_keywords %></td>
                      <td><%= task_profile.emc_category_oid %></td>
                      <td><%= task_profile.primary_question_set_id %></td>
                      <td><%= task_profile.location_specific %></td>
                      <td><%= task_profile.commercial %></td>
                      <td><%= task_profile.view_first_parent_task %></td>
          
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
